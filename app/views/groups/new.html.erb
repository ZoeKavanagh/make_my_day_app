<form action='group#create' method='post'>
  <% @users.each do |user| %>
  <input type='checkbox' name='user' value="<% user.name %>"> <%= user.name %></br>
  <% end %>
</form>

<%= form_for @group do |form| %>
  <%= form.label :name %>
  <%= form.text_area :name %>
  <%= form.submit "Create Group" %>
<% end %>
